body {
  padding-top: 50px;
}
html, body {
	height:100%;
	width: 100%;
}
html { overflow-y: hidden; }

#video1, #video3{ float: left;  overflow: hidden; }

#video2, #video4{ float: right;  overflow: hidden;}

.floating-label {
	position: absolute;
	padding: 0.2em;
	margin-left: 0.2em;
	color: white;
	text-shadow: -1px -1px 0 #000,
    1px -1px 0 #000,
    -1px 1px 0 #000,
    1px 1px 0 #000;
}

.selected {
	color: limegreen;
	font-weight: 800;
}


div {

	animation: bubble 1.0s forwards;
}

.starter-template {
  padding: 40px 15px;
  text-align: center;
}

.infobar {
	height: 100%;
	background-color: #6d6f72;
	padding: 0px;

}

.main{
	height: 100%;

}

.container{
	padding: 0px;
	margin: 0px;
	width: 100%;
	height: 100%;
}

.row{
	height: 100%;

}
.display-as-table {height:100%; width: 100%;}


.video{
	height: 50%;
	width: 50%;
	border: 5px solid #111111;
	padding: 0px;
	
	
}

.videoframe{
	height: 50%;

}



table{
   
   height:100%;
   width: 100%;
}

td {
	background-color: #edbdbd;

}

.feed{
	background-color: #c9d5e8;
}

.circle {
  height: 100%;
  width: 100%;
  background-color: #555;
  border-radius: 50%;
  color: #dcdfe2;
  padding: 15px;
  font-size: xx-large;
 }

 .circle_use{

 	background-color: #7ca8ef;
 }


 .current_ID{
 	height: 13%;
 	width: 100%;
 	margin: 0px;
 }

.ID{
	padding: 11px;
	height: 100%;
	width: 25%;
}

.infoheading{
	height: 5%;
	width: 100%;
	background-color: #383a3d;
	color: #a4a8ad;
	padding-top: 10px;
	margin: 0px;
}

.togglebar{
	height: 15%;
	padding: 0px;
	width: 100%;
	margin: 0 auto;
}

.toggle{
	padding: 0px;
	margin: 0 auto;

	 
}

.switchdiv{
	margin-top: 15px;
	margin-bottom: 15px;

}

#proxswitch{
	margin-bottom: 0px;
}
.box{
	display:inline-block;
	width: 100%;
	height: 100%;
	background-color: black;
}

